main(args: int[][]) {
    // calculate 4^7
    total:int = 0 
    num:int = 2
    a:int = 0; while (a < num) {
    b:int = num; a = a + 1; while (b > 0) {
    c:int = 0; b = b - 1; while (c < num) {
    d:int = num; c = c + 1; while (d > 0) {
    e:int = 0; d = d - 1; while (e < num) {
    f:int = num; e = e + 1; while (f > 0) {
    g:int = 0; f = f - 1; while (g < num) {
         g = g + 1;
         total = total + 1
    }}}}}}}
}


