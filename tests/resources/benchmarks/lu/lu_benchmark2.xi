main(args: int[][]) {
    // calculate 7^7
    total:int = 0 
    a:int = 0; while (a < 7) {
    b:int = 7; a = a + 1; while (b > 0) {
    c:int = 0; b = b - 1; while (c < 7) {
    d:int = 7; c = c + 1; while (d > 0) {
    e:int = 0; d = d - 1; while (e < 7) {
    f:int = 7; e = e + 1; while (f > 0) {
    g:int = 0; f = f - 1; while (g < 7) {
         g = g + 1;
         total = total + 1
    }}}}}}}
}


